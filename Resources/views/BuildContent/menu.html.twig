{% if pageContent is defined %}

	{% set menu = knp_menu_get('BuilderPageBundle:Builder:buildMenu', [], {'menuName': pageContent.content.content|striptags, 'request':request}) %}
	<div class="{{ pageContent.position }} {{ pageContent.content.class }}">
		<nav class="navbar navbar-light navbar-expand-xl">

			<div
				class="navbar-brand">
				{# <span>PAR-CI PAR-L&Agrave;</span> #}
				<a href="{{ path('homepage') }}">
					<img src="{{ asset( '/assets/logo-co-hop.png' ) }}" alt="logo" class="logo w-100"/>
				</a>
			</div>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon">
					<i class="fas fa-bars fa-1x" aria-hidden="true"></i>
				</span>
			</button>
			<div class="collapse navbar-collapse" id="navbarNav">

				{{ knp_menu_render(menu, {'currentClass': 'active', 'template': 'BuilderPageBundle:Menu:knp_menu.html.twig', ancestorClass: 'open'}) }}
			</div>
			<a href="#footer">
				<img src="{{ asset( '/assets/icons/close-envelope.svg' ) }}" class="menu-icon">
			</a>
			{# <svg src="{{ asset( '/assets/icons/close-envelope.svg' ) }}" class="contact-icon"></svg> #}
		</nav>
	</div>
{% else %}
	MENU NON EXISTANT
{% endif %}
